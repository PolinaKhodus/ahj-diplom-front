{
    "env": {
        "browser": true,
        "es2020": true,
	"node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
		"error",
		"LabeledStatement",
		"WithStatement"
        ],
	"no-restricted-globals": "off",
	"no-param-reassign": "off",
	"class-methods-use-this": "off",
	"no-plusplus": "off",
	"consistent-return": "off",
	"import/prefer-default-export": "off",
	"import/no-named-as-default": "off",
        "max-classes-per-file": "off",
	"import/no-cycle": "off"
    }
}
